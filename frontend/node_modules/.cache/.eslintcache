ser,token,loginmessage)=>{\r\n        sessionStorage.setItem('token',JSON.stringify(token));\r\n        sessionStorage.setItem('user',JSON.stringify(user));\r\n        sessionStorage.setItem('message',JSON.stringify(loginmessage));\r\n\r\n        setToken(token);\r\n        setUser(user);\r\n        setMessage(loginmessage);\r\n        navigate('/');\r\n    }\r\n\r\n    const http = axios.create({\r\n        baseURL : \"http://127.0.0.1:8000/api\",\r\n        Headers :{\r\n            \"Content-type\" : \"application/json\"\r\n        }\r\n    });\r\n    const noapi_http = 'http://127.0.0.1:8000/';\r\n    return {\r\n        setToken:saveToken,\r\n        token,\r\n        user,\r\n        message,\r\n        getToken,\r\n        noapi_http,\r\n        http\r\n    }\r\n}","C:\\xampp\\htdocs\\ecom_lr\\frontend\\src\\pages\\users\\Users.component.js",["237"],[],"C:\\xampp\\htdocs\\ecom_lr\\frontend\\src\\pages\\users\\Edituser.component.js",["238"],[],"C:\\xampp\\htdocs\\ecom_lr\\frontend\\src\\pages\\users\\View.component.js",["239","240"],[],"C:\\xampp\\htdocs\\ecom_lr\\frontend\\src\\pages\\categories\\AddCategory.component.js",["241","242","243"],[],"C:\\xampp\\htdocs\\ecom_lr\\frontend\\src\\pages\\categories\\EditCategory.component.js",["244","245","246","247"],[],"import React from \"react\";\r\nimport './category.styles.css';\r\nimport { useNavigate,useParams } from 'react-router-dom';\r\nimport Base from \"../layout/Base.Component\";\r\nimport { useEffect,useState } from \"react\";\r\nimport Baseurl from \"../../components/baseurl/Baseurl.component\";\r\nimport Category from \"./Category.component\";\r\n\r\nexport default function EditCategory(props){\r\n    const [name,setName] = useState('');\r\n    const [image,setImage] = useState('');\r\n    const navigate = useNavigate();\r\n    //Errors\r\n    const [nameError,setNameError] = useState('');\r\n    const [imageError,setimageError] = useState('');\r\n    const [successMsg,setSuccessmsg] = useState('');\r\n\r\n    \r\n    const {http,noapi_http} = Baseurl();\r\n    const {id}= useParams();\r\n\r\n    const hangleChange =(file)=>{\r\n      setImage(file[0]);\r\n    };\r\n\r\n    const editcategory=((e)=>{\r\n      e.preventDefault();\r\n      const data= new FormData();\r\n      data.append('name',name);\r\n      data.append('image',image);\r\n      http.put('/category/update/'+id,data).then((res)=>{\r\n         if(res.data.status==false){\r\n            setNameError(res.data.errors.name);\r\n            setimageError(res.data.errors.image);\r\n            setSuccessmsg(res.data.message);\r\n            return <Category/>\r\n            //navigate('/categories');\r\n         }else{\r\n            setSuccessmsg(res.data.message);\r\n         }\r\n      })\r\n    })\r\n\r\n    // fetch\r\n    const fetchcategory=()=>{\r\n        http.get('/category/edit/'+id).then((res)=>{\r\n            setName(res.data.name);\r\n            setImage(res.data.image); \r\n        });\r\n    }\r\n    useEffect(()=>{\r\n        fetchcategory();\r\n     },[])\r\n    return(\r\n        <Base>\r\n                \r\n          <section className=\"p-3\"> \r\n             <div className=\"card\">\r\n                <h3>Update Category</h3>\r\n                <h2 className=\"text-success\">{successMsg}</h2>\r\n                <div className=\"form\">\r\n                    \r\n                   <form className=\"m-2 p-4\" onSubmit={editcategory}>\r\n                   <div className=\"row\">\r\n                     <div className=\"col-lg-6\">\r\n                     <div className=\"form-group\">\r\n                        <label>Category Name</label>\r\n                         <input className=\"form-control\" type=\"text\" name=\"name\" placeholder=\"Enter Username\"\r\n                         value={name || ''}\r\n                         onChange={e=>setName(e.target.value)} />\r\n                         <span className=\"text-danger\">{nameError}</span>\r\n                     </div>\